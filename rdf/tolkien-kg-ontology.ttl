@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix kg-ont: <http://tolkien-kg.org/ontology/> .
@prefix schema: <http://schema.org/> .

############################
# Classes
############################

kg-ont:Character a owl:Class ;
    rdfs:label "Character" ;
    rdfs:subClassOf schema:Person ;
    rdfs:comment "Personnage de l'univers de Tolkien (personnage fictif tel que défini sur Tolkien Gateway)." .

kg-ont:House a owl:Class ;
    rdfs:label "House" ;
    rdfs:comment "Maison, lignée ou famille noble." .

kg-ont:Race a owl:Class ;
    rdfs:label "Race" ;
    rdfs:comment "Race (Elfe, Homme, Nain, Hobbit, Maia, etc.)." .

kg-ont:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "Lieu géographique (village, ville, région, royaume, etc.)." .

############################
# Object Properties
############################

kg-ont:affiliation a owl:ObjectProperty ;
    rdfs:label "affiliation" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range schema:Organization ;
    rdfs:comment "Organisation, maison ou groupe affilié." .

kg-ont:deathLocation a owl:ObjectProperty ;
    rdfs:label "death location" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range kg-ont:Location ;
    rdfs:comment "Lieu de décès." .

kg-ont:birthLocation a owl:ObjectProperty ;
    rdfs:label "birth location" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range kg-ont:Location ;
    rdfs:comment "Lieu de naissance." .

kg-ont:family a owl:ObjectProperty ;
    rdfs:label "family" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range kg-ont:House ;
    rdfs:comment "Famille ou maison à laquelle le personnage appartient." .

kg-ont:parentage a owl:ObjectProperty ;
    rdfs:label "parentage" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range kg-ont:Character ;
    rdfs:comment "Relation parentale (parent d’un personnage)." .

kg-ont:siblings a owl:ObjectProperty ;
    rdfs:label "siblings" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range kg-ont:Character ;
    rdfs:comment "Frères et sœurs." .

kg-ont:spouse a owl:ObjectProperty ;
    rdfs:label "spouse" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range kg-ont:Character ;
    rdfs:comment "Conjoint(e)." .

kg-ont:children a owl:ObjectProperty ;
    rdfs:label "children" ;
    rdfs:domain kg-ont:Character ;
    rdfs:range kg-ont:Character ;
    rdfs:comment "Enfants." .

kg-ont:steed a owl:ObjectProperty ;
    rdfs:label "steed" ;
    rdfs:domain kg-ont:Character ;
    rdfs:comment "Monture ou destrier." .

kg-ont:weapons a owl:ObjectProperty ;
    rdfs:label "weapons" ;
    rdfs:domain kg-ont:Character ;
    rdfs:comment "Armes utilisées." .

